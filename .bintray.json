{
  "package": {
    "name": "mys-controller-rs",
    "repo": "MySController-rs",
    "subject": "tsathishkumar",
    "desc": "MySensors controller proxy server",
    "vcs_url": "https://github.com/tsathishkumar/MySController-rs.git",
    "github_use_tag_release_notes": true,
    "github_release_notes_file": "RELEASE.txt",
    "licenses": [
      "AGPL-V3"
    ],
    "labels": [
      "mysensors-controller",
      "proxy",
      "ota-update"
    ],
    "public_download_numbers": false,
    "public_stats": false
  },
  "version": {
    "name": "0.1",
    "desc": "Initial version",
    "released": "2018-04-17"
  },
  "files": [
    {
      "includePattern": "target/x86_64-unknown-linux-gnu/release/myscontroller",
      "uploadPattern": "release/myscontroller_x86_64"
    },
    {
      "includePattern": "target/debian/(.*\.deb)",
      "uploadPattern": "pool/main/m/myscontroller-rs/$1",
      "matrixParams": {
        "deb_distribution": "vivid",
        "deb_component": "main",
        "deb_architecture": "amd64"
      }
    }
  ],
  "publish": true
}
